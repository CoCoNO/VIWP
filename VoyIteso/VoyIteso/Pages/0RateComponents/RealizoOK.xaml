<UserControl x:Class="VoyIteso.Pages._0RateComponents.RealizoOK"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    FontFamily="{StaticResource PhoneFontFamilyNormal}"
    FontSize="{StaticResource PhoneFontSizeNormal}"
    Foreground="{StaticResource PhoneForegroundBrush}"
    d:DesignHeight="700" d:DesignWidth="440">

    <Grid x:Name="LayoutRoot" Background="#104571">

        <TextBlock Name="encabezado" Text="¿Se realizó tu aventón con" FontSize="42" HorizontalAlignment="Center" TextWrapping="Wrap" Margin="0,0,0,568" Width="440"></TextBlock>
        <Image Name="DriverImage" Source="../../Images/SMOKE.png" Margin="0,132,10,389"></Image>
        

        <Grid Margin="10,316,10,256">
            <Button Name="si" Content="SI" Margin="0,0,243,40" Click="si_Click" RenderTransformOrigin="0.5,0.5"/>
            <Button x:Name="no" Content="NO" Margin="240,0,0,40" Click="no_Click" RenderTransformOrigin="0.5,0.5"/>

        </Grid>

    </Grid>
</UserControl>